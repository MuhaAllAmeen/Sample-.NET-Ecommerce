@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}

<h1>Category Index</h1>
<div>
    <a asp-controller="Category" asp-action="AddCategory">
    <button class="btn btn-primary"><i class="bi bi-plus-square"></i> &nbsp Create new Category
        </button>
    </a>
</div>
<br />
<table>
    <thead>
        <tr>
            <th>SNo</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
            @foreach(var obj in Model)
            {
                <tr>
                    <td>@obj.Id</td>
                    <td>@obj.Name</td>
                </tr>

            }
    </tbody>
</table>

